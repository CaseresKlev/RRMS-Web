$("#submit").click(function(){
  var g_name=$("#g_name").val();

  var u_name=$("#u_name").val();

  var upass=$("#password").val();
  if (g_name===''|| u_name==='' || upass==='') {
    alert("Please fill all fields!");
  }else {
    $.ajax({
      url:"validate.php",
      type:"POST",
      cache:false,
      data:{
        groupname:g_name,
        uname:u_name,
        password:upass

      },
      success: function (data) {
          //alert(data);

          //$("#result").html(data);
      }



    });

  }



})
